@using mvcCV.Models.entity
@model List<TBLEGİTİM>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br /><br />
<h2>Eğitim Listesi</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>BAŞLIK</th>
        <th>ALT BAŞLIK</th>
        <th>ALT BAŞLIK 2 </th>
        <th>GNO</th>
        <th>TARİH</th>
        <th>SİL</th>
        <th>DÜZENLE</th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.ID</td>
        <td>@item.BASLIK</td>
        <td>@item.ALTBASLIK1</td>
        <td>@item.ALTBASLIK2</td>
        <td>@item.GNO</td>
        <td>@item.TARİH</td>
        <td><a href="/Egitim/EgitimSil/@item.ID" class="btn btn-danger">Sil</a></td>
        <td><a href="/Egitim/EgitimDüzenle/@item.ID" class="btn btn-warning">Düzenle</a></td>
    </tr>
    }

</table>
<a href="/Egitim/EgitimEkle/" class="btn btn-primary">Yeni Eğitim Ekle</a>


